@model UpdateMenuTableViewModel
@{
    ViewData["Title"] = "UpdateMenuTable";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}


<div class="content">
	<div class="container-fluid">
		<h4 class="page-title">Masa İşlemleri</h4>
		<div class="row">
			<div class="col-md-12">
				<form method="post">
					<div class="card">
						<div class="card-header">
							<div class="card-title">Yeni Masa Girişi</div>
						</div>
						<div class="card-body">
							<div class="form-group">
								<input type="hidden" class="form-control" asp-for="Id">
							</div>
							<div class="form-group">
								<label asp-for="Name">Masa Adı</label>
								<input type="text" class="form-control" asp-for="Name">
							</div>
						</div>
						<div class="card-action">
							<button class="btn btn-outline-primary" type="submit">Düzenle</button>
						</div>
					</div>
				</form>
			</div>
		</div>
	</div>
</div>

